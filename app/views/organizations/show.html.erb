<div class="page-header">
  <h1>Showing Organization <%= @organization.name %></h1>
</div>

<h2>People</h2>

<table>
  <thead>
    <tr>
      <th>
        Name
      </th>
    </tr>
  </thead>
  <tbody>
      <% @organization.people.each do |person| %>
        <tr>
          <td>
            <li><%= person.name %></li>
          </td>
        </tr>
      <% end %>
  </tbody>
</table>
